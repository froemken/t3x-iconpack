{
	"compilerOptions": {
		"target": "es2020",
		"allowSyntheticDefaultImports": true,
		"incremental": true,
		"tsBuildInfoFile": "./.cache/tsconfig.tsbuildinfo",
		"module": "es2020",
		"moduleResolution": "node",
		"sourceMap": false,
		"removeComments": false,
		"alwaysStrict": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"pretty": true,
		"baseUrl": "./",
		"outDir": "../../../Resources/Public/JavaScript/v12/",
		"paths": {
			"@quellenform/iconpack.js": [
				"TypeScript/Iconpack.ts"
			],
			"@quellenform/iconpack-modal.js": [
				"TypeScript/IconpackModal.ts"
			],
			"@quellenform/iconpack-wizard.js": [
				"TypeScript/IconpackWizard.ts"
			],
			"@quellenform/iconpack-ckeditor.js": [
				"TypeScript/CKEditor/plugin.ts"
			]
		},
		"typeRoots": [
			"../../node_modules/@types",
			"../../types"
		]
	},
	"include": [
		"./TypeScript/*.ts",
		"./TypeScript/CKEditor/*.ts"
	]
}
